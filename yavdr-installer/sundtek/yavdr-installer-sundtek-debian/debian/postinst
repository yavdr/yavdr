#!/bin/sh

case "$1" in
    configure)
	signal-event sundtek-installer
	cp /tmp/sundtek_installer_100506.sh /usr/share/doc/yavdr-installer-sundtek
	rm -f /tmp/sundtek_installer_100506.sh
       ;;


esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

exit 0

#DEBHELPER#
