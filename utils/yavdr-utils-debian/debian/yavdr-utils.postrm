#!/bin/sh

if [ remove = "$1" ]; then 
   dpkg-divert --package yavdr-startup --remove --rename \
      --divert /etc/firefox/profile/prefs.js.dontremove /etc/firefox/profile/prefs.js
   dpkg-divert --package yavdr-startup --remove --rename \
      --divert /etc/tntnet/tntnet.conf.dontremove /etc/tntnet/tntnet.conf
fi
